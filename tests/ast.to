type TokenType = "symbol" | "number" | "binop"

type Token = {
  line: int,
  col: int,
  value: str,
  toktype: TokenType,
}

type SimpleExpression = {
  etype: "simple",
  t: Token,
}

type BinOp = "+" | "-" | "*" | "/" | "^" | "%" | "&&" | "||"

type BinOpExpression = {
  etype: "binop",
  left: Expression,
  binop: BinOp,
  right: Expression,
}

type Expression =
  SimpleExpression
  | BinOpExpression
